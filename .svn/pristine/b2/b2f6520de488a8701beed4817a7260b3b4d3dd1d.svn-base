	background: url(../wsimages/icon.png) no-repeat 0 0;
	background-size: 100%;
	display: block;
	width: 32px;
	height: 32px;
	margin: 4px auto
}
.tools a:before {
	content: "";
}
.sign-tools a.icon-reset i {
	background-position: center -96px;
}
.tools a.icon-plus i {
	background-position: center -128px;
}
.tools a.icon-minus i {
	background-position: center -160px;
}
.tools a.icon-close i, .sign-tools a.icon-cancel i {
	background-position: center -192px;
}
.sign-tools a.icon-save i, .tools a.icon-confirm i {
	background-position: center -224px;
