.tab-holder .tab {
	display: inline-block;
*display:inline;
*zoom:1;
	margin: 0 auto;
}
.tpl-mod {
	position: relative;
	z-index: 991;
}
#tpl-box {
	border: 1px solid #ddd;
	background: #f5f5f5;
	border-radius: 5px;
	position: absolute;
	z-index: 992;
	top: 50px;
	width: 300px;
	left: -92px;
	box-shadow: 0 5px 5px rgba(0,0,0,0.15)
}
#tpl-box .tpl-list {
	padding-right: 10px;
	padding-top: 10px;
}
#tpl-box .tpl-list li {
	float: left;
	width: 50%;
	border: 0 none;
	padding: 0;
	height: auto;
	line-height: 1
}
#tpl-box .tpl-list li a {
	text-align: center;
	border: 1px solid #ddd;
	background: #fff;
	display: block;
	padding: 10px;
	border-radius: 1px;
	margin-left: 10px;
	margin-bottom: 10px;
	color: #333;
}
#tpl-box .tpl-list li a:hover {
	color: #0275d8;
	border-color: #0275d8
}
.tpl-box i, .tpl-box em {
	border-bottom: #ddd 10px solid;
	border-left: 10px solid transparent;
	border-right: 10px solid transparent;
	position: absolute;
	top: -10px;
	left: 140px;
	height:0;
}
.tpl-box em {
	border-bottom-color: #f5f5f5;
	top: 2px;
	left: -10px;
	
}

