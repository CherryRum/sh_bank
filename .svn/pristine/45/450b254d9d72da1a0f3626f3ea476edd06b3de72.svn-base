.input {
	border: 1px solid #ccc;
	border-radius: 3px;
	outline: 0 none;
	padding: 5px;
	line-height: 26px;
	height: 26px;
	vertical-align: middle;
	display: inline-block;
}
.textarea {
	border: 1px solid #ccc;
	border-radius: 3px;
	outline: 0 none;
	padding: 5px;
	display: inline-block;
	height: auto;
	vertical-align: middle;
}
input[type="checkbox"] {
	vertical-align: middle;
	border: 1px solid #ccc;
	border-radius: 3px;
	width: 16px;
	height: 16px;
	background: #fff
}
.select {
	border: 1px solid #ccc;
	border-radius: 3px;
	outline: 0 none;
	line-height: 38px;
	height: 38px;
	vertical-align: middle;
}
.input:focus, .select:focus {
	border-color: #0095ff;
	box-shadow: 0 0 2px 2px rgba(0,222,225,.20)
}
.input-error {
	border-color: #FB383B;
	background-color: #FFE8E8;
}
