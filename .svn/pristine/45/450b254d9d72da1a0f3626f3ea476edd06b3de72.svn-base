button {
	font-family: "Classic Grotesque W01", "Hiragino Sans GB", "STHeiti", "Microsoft YaHei", "WenQuanYi Micro Hei", Arial, SimSun, sans-serif;
}
.btn {
	background: #e6e6e6;
	border: 1px solid rgb(202, 202, 202);
	border-radius: 3px;
	background-image: -moz-linear-gradient( 90deg, rgb(230,230,230) 0%, rgb(255,255,255) 100%);
	background-image: -webkit-linear-gradient( 90deg, rgb(230,230,230) 0%, rgb(255,255,255) 100%);
	background-image: -ms-linear-gradient( 90deg, rgb(230,230,230) 0%, rgb(255,255,255) 100%);
	display: inline-block;
	width: auto;
	height: 38px;
	min-width: 60px;
	padding: 0 10px;
	border-radius: 3px;
	text-align: center;
	font-weight: 700;
	font-size: 14px;
	line-height: 38px;
	outline: 0 none;
	color: #333;
	vertical-align: middle;
	text-decoration: none;
	overflow: visible;
}
.btn[disabled] {
	border-color: #ddd;
	color: #999
}
.red-btn {
	background: #f2373b;
	background-image: -moz-linear-gradient( 90deg, rgb(223,48,51) 0%, rgb(231,70,73) 100%);
	background-image: -webkit-linear-gradient( 90deg, rgb(223,48,51) 0%, rgb(231,70,73) 100%);
	background-image: -ms-linear-gradient( 90deg, rgb(223,48,51) 0%, rgb(231,70,73) 100%);
	border: 0 none;
	display: inline-block;
	width: auto;
	height: 38px;
	min-width: 60px;
	padding: 0 10px;
	border-radius: 3px;
	text-align: center;
	font-weight: 700;
	font-size: 14px;
	line-height: 38px;
	outline: 0 none;
	color: #FFF;
	vertical-align: middle;
	text-decoration: none;
	overflow: visible;
}
.green-btn {
	background: #009944;
	border: 0 none;
	display: inline-block;
	width: auto;
	height: 38px;
	min-width: 60px;
	padding: 0 10px;
	border-radius: 3px;
	text-align: center;
	font-weight: 700;
	font-size: 14px;
	line-height: 38px;
	outline: 0 none;
	color: #FFF;
	vertical-align: middle;
	text-decoration: none;
	overflow: visible;
}
.red-border-btn {
	background: #fff;
	border: 1px solid #f2373b;
	display: inline-block;
	width: auto;
	height: 38px;
	min-width: 60px;
	padding: 0 10px;
