	text-align: center;
	font-weight: 700;
	font-size: 14px;
	line-height: 38px;
	outline: 0 none;
	color: #f2373b;
	vertical-align: middle;
	text-decoration: none;
	overflow: visible;
}
.blue-btn {
	background: #007bd2;
	background-image: -moz-linear-gradient( 90deg, rgb(0,123,210) 0%, rgb(47,169,255) 100%);
	background-image: -webkit-linear-gradient( 90deg, rgb(0,123,210) 0%, rgb(47,169,255) 100%);
	background-image: -ms-linear-gradient( 90deg, rgb(0,123,210) 0%, rgb(47,169,255) 100%);
	border: 0 none;
	display: inline-block;
	width: auto;
	height: 40px;
	min-width: 60px;
	padding: 0 10px;
	border-radius: 3px;
	text-align: center;
	font-weight: 700;
	font-size: 14px;
	line-height: 40px;
	outline: 0 none;
	color: #FFF;
	vertical-align: middle;
	text-decoration: none;
	overflow: visible;
}
a.red-btn, a.blue-btn, a.green-btn {
