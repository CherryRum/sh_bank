@charset "utf-8";
/* CSS Document */
html {
	background: #f3f3f3;
	
	font-size: 14px;
	line-height: 28px;
}
.page {
	border: 1px solid #e2e2e2;
	width: 640px;
	padding: 45px;
	margin: 30px auto;
	background: #fff;
	position: relative;
	z-index: 0;
}
.contract {
	text-align: center;
}
.contract img {
	width: 638px;
	border: 1px solid #868686;
}
.contract .copyright {
	width: 100%;
	height: 100%;
	position: absolute;
	z-index: 1;
	background: url(../images/check/icon-copyright.png) repeat-y center top;
	top: 0;
	left: 0
}
.contract .seal {
	width: 385px;
	height: 85px;
	position: absolute;
	z-index: 2;
	background: url(../images/check/icon-seal.png) repeat-y center top;
	right: 65px;
	top: 65px;
}
.fr {
	float: right;
	display: inline
}
.table {
	border: 1px solid #868686;
	border-collapse: collapse;
}
.table td, table th {
	border: 1px solid #868686;
	padding: 5px;
}
.table th {
	font-size: 16px;
	font-weight: 700
}
.signer p {
	border-top: 1px solid #868686;
}
.signer p.first-child, .signer p:first-child {
	border: 0 none
}
.fixed {
	position: fixed;
	right: 5px;
	top: 50%;
	margin-top: -75px;
	width: 50px;
	text-align: center;
	background: #aaa;
	background: rgba(0,0,0,0.3);
}
.fixed a {
	text-decoration: none;
	text-indent: -200px;
	display: block;
	width: 50px;
	height: 50px;
	line-height: 50px;
	overflow: hidden
}
.fixed a:hover {
	background: #2f3239;
	background: rgba(0,0,0,0.8);
	text-indent: 0;
	color: #868686
}
.fixed .icon-print {
	background: url(../images/check/icon-print.png) no-repeat center center
}
.fixed .icon-download {
	background: url(../images/check/icon-download.png) no-repeat center center
}
.fixed .icon-backtop {
	background: url(../images/check/icon-backtop.png) no-repeat center center;
	line-height: 20px;
	padding: 5px;
	width: 40px;
	height: 40px;
}
.gray{ color:gray}

@media print {
.fixed {
	display: none;
}
.page {
	min-height: 970px
}
.contract {
	top: 30px;
}
}

