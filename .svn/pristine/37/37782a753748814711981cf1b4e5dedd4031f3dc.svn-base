@charset "utf-8";
.ht-header h1{ text-align:left}
.ht-info{ width:100%}
.dialog-overlay {
	position: fixed;
	z-index: 998;
	width: 100%;
	height: 100%;
	filter:progid:DXImageTransform.Microsoft.gradient(enabled='true',startColorstr='#7F000000', endColorstr='#7F000000');
	background: rgba(0,0,0,0.50);
	left: 0;
	top: 0
}
.dialog {
	width: 500px;
	background: #fff;
	position:absolute;
	z-index: 999;
	left: 50%;
	margin-left: -250px;
	top: 50%;
	margin-top:-250px
}
.dialog .animation {
	height: 400px;
	background: #232a3c;
	padding:20px;
	color:#fff;
	text-align:center
}
.dialog .animation i{ width:140px; height:140px; display:block;margin:50px auto}
.dialog .animation i.icon-right{background: url(../images/butt/icon-right.png) no-repeat center center}
.dialog .animation i.icon-wrong{background: url(../images/butt/icon-wrong.png) no-repeat center center}
.dialog .dialog-content{ font-size:16px}
.dialog .dialog-content p.fs24{ font-size:24px}
.dialog .progress {
	height: 8px;
	background: url(../images/butt/progress.png) repeat-x;
}
.dialog .status {
	padding:10px;
}
.dialog .checking {
	background: #232a3c url(../images/butt/checking.gif) no-repeat center center
}
.dialog .signing {
	background: #232a3c url(../images/butt/signing.gif) no-repeat center center
}
.dialog .progress-bar {
	background: url(../images/butt/progress-bar.gif) repeat-x;
	height: 8px;
}
.dialog .status li{ width:33.33%; float:left; text-align:center;padding:10px 0;}
.dialog .status li i{ display:block; width:50px; height:50px; margin:10px auto}
.dialog .status li i.icon-link{background: url(../images/butt/icon-link.png) no-repeat center center}
.dialog .status li i.icon-link::before{ content:''}
.dialog .status li i.icon-validity{background: url(../images/butt/icon-validity.png) no-repeat center center}
.dialog .status li i.icon-crl{background: url(../images/butt/icon-crl.png) no-repeat center center}
.dialog .status li.active i.icon-link{background-image: url(../images/butt/icon-link.gif)}
.dialog .status li.active i.icon-validity{background-image: url(../images/butt/icon-validity.gif)}
.dialog .status li.active i.icon-crl{background-image: url(../images/butt/icon-crl.gif)}
.dialog .status li.passed i.icon-link{background-image: url(../images/butt/icon-passed-link.png)}
.dialog .status li.passed i.icon-validity{background-image: url(../images/butt/icon-passed-validity.png)}
.dialog .status li.passed i.icon-crl{background-image: url(../images/butt/icon-passed-crl.png)}

.dialog .status li.failed i.icon-link{background-image: url(../images/butt/icon-failed-link.png)}
.dialog .status li.failed i.icon-validity{background-image: url(../images/butt/icon-failed-validity.png)}
.dialog .status li.failed i.icon-crl{background-image: url(../images/butt/icon-failed-crl.png)}


.dialog .dialog-title{ font-size:14px; color:#fff; text-align:left}
.dialog .percent{ color:#007ec6; font-size:45px; text-align:right}
.dialog .btns{padding:20px 0; height:40px; text-align:center}

.seal-list {
	position: fixed;
	right: -245px;
	top: 250px;
	width: 240px;
	background: #fff;
	border: 1px solid #ddd;
	border-radius: 5px;
	z-index: 4;
	-webkit-transition: all linear .5s;
	-moz-transition: all linear .5s;
	transition: all linear .5s;
}
.seal-show {
	right: 5px
}
.seal-list .seal-model {
	height: 260px;
	overflow-y: auto;
}
.seal-model li {
	width: 44%;
	*width: 40%;
	float: left;
	padding: 8px 3%;
	text-align: center;
	height: 114px;
	cursor: pointer;
}
.seal-model li.current {
	background: rgba(0,0,0,0.10)
}
.seal-model li .img {
	height: 90px;
	width: 100%;
	overflow: hidden
}
.seal-model li .txt {
	width: 100%;
	white-space: nowrap;
	height: 24px;
	line-height: 24px;
	overflow: hidden;
	text-overflow: ellipsis
}
.seal-model li .img img {
    width: 86%;
    height: auto;
}
.seal-list .tab {
	position: absolute;
	top: -33px;
	background: #fff;
	height: 30px;
	line-height: 30px;
	width: 242px;
	left: -1px;
}
.seal-list .tab li {
	width: 33.3%;
	*width: 33%;
	float: left;
	text-align: center;
}
.seal-list .tab li a {
	display: block;
	border: 1px solid #ddd;
	color: #666;
	text-decoration: none;
	border-bottom: 2px solid #F64343
}
.seal-list .tab li.current a {
	border-top-color: #F64343;
	border-left-color: #F64343;
	border-right-color: #F64343;
}
.d-inline-block {
	display: inline-block;
*display:inline;
*zoom:1
}


.rbar {
	right: 5px;
	bottom: 80px;
	width: 40px;
	z-index: 3;
	position:fixed;
}
.rbar a {
	margin-bottom: 1px;
}
.pencil {
	width: 40px;
	height: 40px;
	display: block;
	background: #F64343;
}
.seal {
	width: 40px;
	height: 40px;
	display: block;
	background: #F64343;
}
.backtop-holder {
	width: 40px;
	height: 40px;
}
.backtop {
	width: 40px;
	height: 40px;
	display: block;
	background: #444;
}
.backtop em, .pencil em, .seal em {
	display: none;
}
.backtop i {
	background: url(../images/scroll-top.png) no-repeat center center;
	height: 40px;
	width: 40px;
	line-height: 40px;
	display: inline-block;
*display:inline;
*zoom:1;
}
.pencil i {
	background: url(../images/pencil.png) no-repeat center center;
	height: 40px;
	width: 40px;
	line-height: 40px;
	display: inline-block;
*display:inline;
*zoom:1;
}
.seal i {
	background: url(../images/seal.png) no-repeat center center;
	height: 40px;
	width: 40px;
	line-height: 40px;
	display: inline-block;
*display:inline;
*zoom:1;
}
.backtop:hover, .pencil:hover, .seal:hover {
	text-decoration: none;
	background: #F64343
}
.backtop:hover em, .pencil:hover em, .seal:hover em {
	display: block;
	color: #FFF;
	width: 30px;
	height: 30px;
	padding: 5px;
	line-height: 15px;
	text-align: center
}
.backtop:hover i, .pencil:hover i, .seal:hover i {
	display: none
}
.draggable {
	position: absolute;
	border: 2px dashed #ddd;
	cursor: move;
	z-index: 98;
	width:auto;
	height:auto; 
}
.draggable i {
	position: absolute;
	right: -45px;
	top: 0;
	width: 30px;
	height: 30px;
	line-height: 30px;
	background: #fff url(../images/icon-del.png) no-repeat center center;
	text-indent: -9999px;
	border-radius: 30px;
	border: 1px solid #eee;
	opacity: .5
}
.draggable i:hover {
	cursor: pointer;
	opacity: 1;
	border-color: #F64343
}
.draggable:hover {
	background: rgba(0,0,0,0.2);
	border-color: #666;

}
.ui-resizable-se {
	background: url(../images/resize.png)
}