package com.mmec.centerService.contractModule.entity;

import com.mmec.centerService.userModule.entity.IdentityEntity;

public class ContractImgBean
{
	private ContractEntity contractEntity;
	private ContractPathEntity contractPathEntity;
	private IdentityEntity identity;
	public ContractEntity getContractEntity()
	{
		return contractEntity;
	}
	public void setContractEntity(ContractEntity contractEntity)
	{
		this.contractEntity = contractEntity;
	}
	public ContractPathEntity getContractPathEntity()
	{
		return contractPathEntity;
	}
	public void setContractPathEntity(ContractPathEntity contractPathEntity)
	{
		this.contractPathEntity = contractPathEntity;
	}
	public IdentityEntity getIdentity()
	{
		return identity;
	}
	public void setIdentity(IdentityEntity identity)
	{
		this.identity = identity;
	}
	public ContractImgBean(ContractEntity contractEntity,ContractPathEntity contractPathEntity,IdentityEntity identity)
	{
		this.contractEntity = contractEntity;
		this.contractPathEntity = contractPathEntity;
		this.identity = identity;
	}
}
