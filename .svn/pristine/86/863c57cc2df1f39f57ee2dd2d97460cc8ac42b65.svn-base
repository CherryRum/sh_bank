body {
	font: 14px/1.5 "微软雅黑";
	background: #efeff0;
}
.page {
	background: #fff;
	width: 950px;
	text-align: center;
	margin: 20px auto;
	box-shadow: 0 0 15px rgba(0,0,0,0.10)
}
.page-01 {
	padding: 50px 0;
	margin-top: 40px;
	background: #fff;
}
.page-01 h1 {
	font-size: 38px;
	text-align: center
}
.page-01 h2 {
	font-size: 14px;
	text-align: right;
	margin-bottom: 50px
}
a {
	color: #007BD1;
	text-decoration: none
}
.data {
	border: 1px solid #D7D7D7;
	line-height: 30px;
	margin: 50px;
	width: 580px;
}
.data th {
	text-align: left;
	padding-left: 38px;
}
.data .padding {
	padding: 50px;
}
.hr {
	border: 0 none;
	border-top: 3px solid #cd141c;
}
.red {
	color: red;
	font-size: 12px
}
dl {
	margin-bottom: 20px
}
dt, dd {
	line-height: 20px
}
dt {
	font-weight: bold
}
dd {
	margin-left: 10px
}
.dl-horizontal {
 *zoom: 1;
	background: #fff;
	margin: 0 10px 2px
}
.dl-horizontal:before, .dl-horizontal:after {
	display: table;
	line-height: 0;
	content: ""
}
.dl-horizontal:after {
	clear: both
}
.dl-horizontal dt {
	float: left;
	width: 160px;
	padding: 10px;
	overflow: hidden;
	clear: left;
	text-align: right;
	text-overflow: ellipsis;
	white-space: nowrap
}
.dl-horizontal dd {
	margin-left: 200px;
	padding: 10px;
	text-align: left
}
.dl-horizontal h3 {
	margin: 0;
	font-weight: 100
}
.gray {
	color: gray;
}
.print {
	width: 70px;
	height: 27px;
	line-height: 27px;
	display: inline-block;
	background: url(../images/print.png) -1px -1px no-repeat;
	vertical-align: middle;
	margin-left: 10px;
}
.print:hover {
	background-position: -1px -36px;
}
.top {
	position: fixed;
	bottom: 5px;
	right: 10px;
	width: 39px;
	line-height: 53px;
	height: 53px;
	background: url(../../images/print.png) 0 -70px no-repeat;
}
.top:hover {
	background-position: -47px -70px;
}
.title {
	height: 50px;
	line-height: 50px;
	padding-left: 20px;
	font-size: 16px;
	color: #CDCDCD;
	font-weight: 700;
	width: 840px;
	margin: 0 auto;
}
.footer {
	width: 840px;
	margin: 0 auto;
	height: 55px;
	color: #C5C5C5;
	font-size: 12px;
	line-height: 55px;
	text-align: center
}
.padding a {
	display: inline-block;
	width: 45%;
	text-align: left;
 *zoom:1;
	margin: 10px 0;
}
.popup {
	background: #000;
	filter: alpha(opacity=50);
	opacity: 0.5;
	display: none;
	position: fixed;
	top: 0px;
	left: 0px;
	z-index: 100;
}
.p_b5 {
}
.p_b5_title {
	font-size: 13px;
	color: #343434;
	font-weight: bold;
	padding: 9px;
}
.p_b5_title span {
	font-weight: normal;
	color: #707070;
	font-size: 12px;
}
.p_b5 .p_bind_2 {
	width: 100%;
	margin-top: 15px;
}
.p_tile {
	height: 25px;
	line-height: 25px;
	font-weight: bold;
	padding-left: 5%;
	font-size: 14px;
	font-family: Arial, "宋体";
	color: #006bae;
	width: 95%;
	text-align: left;
	background-image: -o-linear-gradient(top, #eef9ff, #d1eeff);
	background-image: -ms-linear-gradient(top, #eef9ff, #d1eeff);
	background-image: -moz-linear-gradient(top, #eef9ff, #d1eeff);
	background-image: -webkit-linear-gradient(top, #eef9ff, #d1eeff);
	background-image: linear-gradient(top, #eef9ff, #d1eeff );
 filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#eef9ff', endColorstr='#d1eeff', GradientType='0');
}
.p_bind_2 {
	text-align: right;
	line-height: 42px;
	border-top: 1px solid #bae4ff;
}
.p_b_ok {
	width: 55px;
	height: 23px;
	line-height: 23px;
	margin: 5px 15px;
}
.all_btn {
	background: #ff6d00;
	text-align: center;
	border: 1px solid #ff4e00;
	display: inline-block;
	color: #fff;
	border-radius: 5px;
}
.all_btn:hover {
	color: #fff;
	text-decoration: none;
	border: 1px solid #cf7523;
	background-image: -moz-linear-gradient(top, #ffc057, #f5870f);
	background-image: -webkit-gradient(linear, left top, left bottom, color-stop(0, #ffc057), color-stop(1, #f5870f));
filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#ffc057', endColorstr='#f5870f', GradientType='0');
}
.p_box {
	width: 600px;
	height: auto;
	display: none;
	border: 1px solid #94cae9;
	background: #fff;
	position: fixed;
	font-size: 14px;
	z-index: 101;
}
#mainContract img {
	max-width: 840px
}
.mb20 {
	margin-bottom: 20px;
}
.mt20 {
	margin-top: 20px;
}
.mb20 p {
	margin: 0;
	line-height: 28px
}
.inblock {
	display: inline-block;
*display:inline;
*zoom:1;
	width: 70px;
	text-align: right;
}
.mr20 {
	margin-right: 20px;
}
.fixed {
	position: fixed;
	right: 5px;
	top: 50%;
	margin-top: -75px;
	width: 50px;
	text-align: center;
	background: #fff;
	background: rgba(255,255,255,.8);
}
.fixed a {
	text-decoration: none;
	text-indent: -200px;
	display: block;
	width: 50px;
	height: 50px;
	line-height: 50px;
	overflow: hidden
}
.fixed a:hover {
	background: rgb(255,255,255);
	text-indent: 0;
	color: #333
}
.fixed .icon-print {
	background: url(../images/icon-print.png) no-repeat center center
}
.fixed .icon-backtop {
	background: url(../images/icon-backtop.png) no-repeat center center;
	line-height: 20px;
	padding: 5px;
	width: 40px;
	height: 40px;
}

@media print {
.fixed {
	display: none;
}
}
.page-outer {
	border: 4px solid #005bac;
	width: 802px;
	margin: 0 auto;
}
.page-inner {
	border: 1px solid #005bac;
	margin: 5px;
	background: #efefef;
}
.sign-seal {
	border: 2px solid #005bac;
	background: url(../images/zip-line.png) repeat center center;
	position: relative;
	z-index: 0;
	margin: 10px;
	width: 340px;
}
.sign-seal .sign-inner {
	width: 240px;
	border: 1px solid #005bac;
	margin: 5px;
	font-size: 12px;
	padding: 5px 15px 5px 70px;
	background: #fffff0 url(../images/zip-seal.png) no-repeat 20px center;
}
.sign-seal i {
	position: absolute;
	z-index: 1;
	right: 0;
	top: 0;
	width: 59px;
	height: 50px;
}
.sign-seal i.icon-company {
	background: url(../images/zip-company.png) no-repeat;
}
.sign-seal i.icon-personal {
	background: url(../images/zip-personal.png) no-repeat;
}
.timestamp {
	width: 133px;
	height: 111px;
	background: url(../images/timestamp.png) no-repeat;
	position: relative;
	z-index: 0;
}
.timestamp p {
	position: absolute;
	z-index: 1;
	color: #3e3a39;
	font-size: 10px;
	width: 133px;
	text-align: center;
	font-weight: 700;
	top: 53px;
	transform: scale(.9);
}
.zip-data {
	border: 1px solid red;
	background: #fff url(../images/zip-data-bg.png) no-repeat center center;
	margin: 10px;
}
.padding{ padding:10px;}