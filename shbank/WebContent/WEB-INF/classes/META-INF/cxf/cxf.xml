<?xml version="1.0" encoding="UTF-8"?>  
<beans xmlns="http://www.springframework.org/schema/beans"  
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"  
       xmlns:jaxws="http://cxf.apache.org/jaxws"  
       xsi:schemaLocation="  
             http://www.springframework.org/schema/beans  
             http://www.springframework.org/schema/beans/spring-beans.xsd  
             http://cxf.apache.org/jaxws   
             http://cxf.apache.org/schemas/jaxws.xsd">  
  
	<jaxws:endpoint   
             id="CommonBussiness"  
             implementor="com.mmec.webservice.service.impl.CommonBussinessImpl"  
             address="/Common"/>
             
	<jaxws:endpoint   
             id="AdditionBussiness"  
             implementor="com.mmec.webservice.service.impl.AdditionBussinessImpl"  
             address="/Addition"/>
             
	<jaxws:endpoint   
             id="SpecializationBussiness"  
             implementor="com.mmec.webservice.service.impl.SpecializationBussinessImpl"  
             address="/Specialization"/>
	             
 	<jaxws:endpoint   
             id="MultiPartyBussiness"  
             implementor="com.mmec.webservice.service.impl.MultiPartyBussinessImpl"  
             address="/MultiParty"/>
    
    <!-- 云签本地化 -->         
    <jaxws:endpoint   
             id="LocalYunsignBussiness"  
             implementor="com.mmec.webservice.service.impl.LocalYunsignBussinessImpl"  
             address="/LocalYunsign"/>
             
    <jaxws:endpoint   
             id="InternalBussiness"  
             implementor="com.mmec.webservice.service.impl.InternalBussinessImpl"  
             address="/Internal"/>         
</beans>

